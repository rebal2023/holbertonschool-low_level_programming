#include<stdio.h>
#include<main.h>
/**
*main - program that multiplies two numbers
*@argc: nb of arguments
*@argv: array of arguments
*Return: 0
*/
int main(int argc, char **argv)
{
	int x = 0;
	int y = 0;
	int mul = 0;

	if (argc == 3)
	{
		x = _atoi(argv[1]);
		y = _atoi(argv[2]);
		mul = x * y;
		printf("%d\n", mul);
	}
	else
	{
		printf("Error\n");
		return (1);
	}
	return (0);
}
